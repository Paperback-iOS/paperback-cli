{
  "name": "paperback-cli",
  "description": "Paperback CLI for common tools",
  "version": "2.0.0-alpha.7",
  "author": "Faizan Durrani",
  "bin": {
    "paperback": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/FaizanDurrani/paperback-cli/issues"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@oclif/test": "^1.2.7",
    "@types/browserify": "^12.0.36",
    "@types/chai": "^4.2.14",
    "@types/google-protobuf": "^3.15.5",
    "@types/ip": "^1.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.56",
    "@types/vinyl-source-stream": "^0.0.30",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^9.1.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/FaizanDurrani/paperback-cli#readme",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "paperback",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FaizanDurrani/paperback-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && ./src/devtools/generate-ts.sh && tsc -b && oclif-dev manifest && oclif-dev readme && node ./bin/prepack-script",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@types/listr": "^0.14.2",
    "@types/shelljs": "^0.8.8",
    "@types/update-notifier": "^5.0.0",
    "browserify": "^17.0.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.5.1",
    "google-protobuf": "^3.18.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2",
    "ip": "^1.1.5",
    "listr": "^0.14.3",
    "paperback-extensions-common": "^5.0.0-alpha.4",
    "pug": "^3.0.1",
    "shelljs": "^0.8.4",
    "tsify": "^5.0.2",
    "tslib": "^1.14.1",
    "update-notifier": "^5.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
